		<Label layoutX="51.0" layoutY="232.0" text="Theme: " />
		<Label layoutX="308.0" layoutY="39.0" styleClass="big" text="Options">
			<font>
				<Font size="16.0" />
			</font>
		</Label>
		<Button layoutX="108.0" layoutY="413.0" mnemonicParsing="false"
			onAction="#cancelButtonAction" text="Cancel" />
		<Button layoutX="528.0" layoutY="413.0" mnemonicParsing="false"
			onAction="#applyButtonAction" prefHeight="28.0" prefWidth="59.0"
			text="Apply" />
	</children>
